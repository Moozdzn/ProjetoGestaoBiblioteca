<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="TOP_CENTER" prefHeight="75.0" prefWidth="458.0" text="Controlo de Disponibilidade de Livros">
         <font>
            <Font size="26.0" />
         </font></Label>
      <Label text="Já possui conta?" />
      <Button mnemonicParsing="false" onAction="#login" prefHeight="16.0" prefWidth="62.0" text="Login" />
      <Label text="Se não, crie uma agora" />
      <Button mnemonicParsing="false" onAction="#create" text="Sign Up" textAlignment="RIGHT" />
   </children>
</VBox>
